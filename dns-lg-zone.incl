        IN   TXT     "http://dnslg.generic-nic.net/"
        IN   TXT     "http://dns-lg.nlnetlabs.nl/"
        IN   TXT     "http://dns-lg.tetaneutral.net/"
        IN   TXT     "http://dns-lg.vs.uni-due.de/"
